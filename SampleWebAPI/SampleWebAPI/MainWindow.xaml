<Window x:Class="SampleWebAPI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SampleWebAPI"
        mc:Ignorable="d"
        Title="MainWindow" Height="480" Width="400">
    <Grid>
        <DockPanel LastChildFill="True" Grid.Column="0" Grid.ColumnSpan="2" Margin="0,0,0,0.5">
            <StackPanel DockPanel.Dock="Top">
                <TextBlock Text="Zipcode : "/>
                <TextBlock x:Name="ZipcodeTextBox" Margin="10, 0, 10, 0" FontSize="50" Text="1000000"/>
                <TextBox x:Name="TimerTextBox" Text="2020/07/07 21:00:00"/>
            </StackPanel>
            <DockPanel DockPanel.Dock="Top">
                <Button Click="Button_Click_1">
                    <TextBlock Text="TimeSetting"/>
                </Button>
                <Button Content="Exec Web API" FontSize="50" Click="Button_Click"/>
            </DockPanel>
            <TextBlock Text="Result : "  DockPanel.Dock="Top"/>
            <TextBox x:Name="ResultTextbox" DockPanel.Dock="Top" Margin="10, 0, 10, 0"/>
        </DockPanel>
    </Grid>
</Window>
